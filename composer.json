{
    "name": "ienroll/school-enrollment-management",
    "description": "School Enrollment Management System with Email Verification",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "phpmailer/phpmailer": "^6.8",
        "vlucas/phpdotenv": "^5.5",
        "firebase/php-jwt": "^6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "iEnroll\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "setup": "php setup.php"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
